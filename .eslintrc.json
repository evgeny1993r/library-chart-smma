{
  "extends": ["airbnb", "airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/prefer-default-export": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-param-reassign": "off",
    "no-continue": "off",
    "import/no-cycle": "off",
    "max-len": ["error", { "code": 120 }]
  }
}
